<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/publish_tasks_button_radius" />
            <solid android:color="@color/publish_task_button" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/publish_tasks_button_radius" />
            <solid android:color="@color/publish_task_button_pressed" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/publish_tasks_button_radius" />
            <solid android:color="@color/publish_task_button_disable" />
        </shape>
    </item>
</selector>